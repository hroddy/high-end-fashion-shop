{
  "name": "high-end-fashion-shop",
  "version": "1.0.0",
  "description": "A high end fashion shop",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "heroku-postbuild": "npm run seed",
    "dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjos7/high-end-fashion-shop.git"
  },
  "keywords": [
    "fashion",
    "ecommerce",
    "shop",
    "luxury",
    "lifestyle"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^6.4.0"
  }
}
